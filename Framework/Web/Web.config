<configuration>
  <configSections>
    <!-- Log4net BEGIN -->
    <section name="log4net" type="log4net.Config.Log4NetConfigurationSectionHandler, log4net" />
    <!-- Log4net END -->
  </configSections>
  <appSettings>
    <add key="webpages:Version" value="3.0.0.0" />
    <add key="webpages:Enabled" value="false" />
    <add key="ClientValidationEnabled" value="true" />
    <add key="UnobtrusiveJavaScriptEnabled" value="true" />

    <!-- Log4net BEGIN -->
    <!-- Auto Combine <root>//path -->
    <add key="LoggerDebugPath" value="Logs//Debug//" />
    <add key="LoggerInfoPath" value="Logs//Info//" />
    <add key="LoggerWarnPath" value="Logs//Warn//" />
    <add key="LoggerErrorPath" value="Logs//Error//" />
    <add key="LoggerFatalPath" value="Logs//Fatal//" />
    <!-- Log4net END -->
    <!-- Resource directory BEGIN -->
    <!-- Auto Combine <root>//path -->
    <add key="ResourceDirectory" value="Resource" />
    <!-- Resource directory END-->

    <!-- Send mail -->
    <add key="Email" value="sts.sendmail.test@gmail.com" />
    <add key="Password" value="sendmail@123" />
    <add key="EnableSsl" value="true" />
    <add key="Host" value="smtp.gmail.com" />
    <add key="Port" value="587" />
    
    <!-- Url -->
    <add key="RootUrl" value="http://localhost:7660/" />
    <add key="OuoUrl" value="http://ouo.io/s/fxsYE8gM?s=" />
    <add key="AdfUrl" value="http://adf.ly/12286525/" />
    
  </appSettings>
  <connectionStrings>
    <!-- LOCAL -->
    <add name="FrameworkContext" connectionString="Data Source=.;Initial Catalog=Getlink;Persist Security Info=True;User ID=sa;Password=123456;MultipleActiveResultSets=True" providerName="System.Data.SqlClient" />
  </connectionStrings>
  <!-- Log4net BEGIN -->
  <log4net>
    <appender name="FileAppender" type="log4net.Appender.FileAppender">
      <file type="log4net.Util.PatternString" value="%property{logPath}%date{yyyy.MM.dd}.log" />
      <encoding value="utf-8" />
      <appendToFile value="true" />
      <layout type="log4net.Layout.PatternLayout">
        <conversionPattern value="%date [%thread] %level: %newline%message%exception%newline%newline" />
      </layout>
    </appender>
    <root>
      <level value="ALL" />
      <appender-ref ref="FileAppender" />
    </root>
  </log4net>
  <!-- Log4net END -->
  <!--
    For a description of web.config changes see http://go.microsoft.com/fwlink/?LinkId=235367.

    The following attributes can be set on the <httpRuntime> tag.
      <system.Web>
        <httpRuntime targetFramework="4.5.1" />
      </system.Web>
  -->
  <system.web>
    <compilation debug="true" targetFramework="4.5.1">
      <assemblies>
        <add assembly="System.Runtime, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b03f5f7f11d50a3a" />
      </assemblies>
    </compilation>
    <!-- Request length BEGIN -->
    <!-- 50MB in kilobytes, default is 4096 or 4MB-->
    <httpRuntime targetFramework="4.5" requestValidationMode="2.0" maxRequestLength="51200" />
    <!-- Request length END -->

    <!-- Custom error BEGIN-->
    <customErrors mode="Off" />
    <!-- Custom error END-->
  </system.web>
  <system.webServer>
    <security>
      <!-- Request length BEGIN -->
      <requestFiltering>
        <!-- 50MB in bytes, default is 30000000 or approx. 28.6102 Mb-->
        <requestLimits maxAllowedContentLength="52428800" />
      </requestFiltering>
      <!-- Request length END -->
    </security>
    <modules runAllManagedModulesForAllRequests="true" />
    <staticContent>
      <remove fileExtension=".woff2" />
      <mimeMap fileExtension=".woff2" mimeType="application/font-woff2" />
    </staticContent>
    <handlers>
      <remove name="ExtensionlessUrlHandler-Integrated-4.0" />
      <remove name="OPTIONSVerbHandler" />
      <remove name="TRACEVerbHandler" />
      <add name="ExtensionlessUrlHandler-Integrated-4.0" path="*." verb="*" type="System.Web.Handlers.TransferRequestHandler" preCondition="integratedMode,runtimeVersionv4.0" />
    </handlers>
    <directoryBrowse enabled="false" />
  </system.webServer>
